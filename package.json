{
  "name": "ffwiki",
  "version": "0.1.0",
  "main": "server/index.js",
  "author": "An Cao",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development npm-run-all --parallel server client",
    "server": "npm-run-all --parallel server:watch server:start",
    "server:watch": "webpack --config webpack.server.js --watch",
    "server:start": "nodemon dist/server.js",
    "client": "webpack --config webpack.client.js --watch",
    "build": "rimraf dist/ && NODE_ENV=production npm-run-all --sequential build:client build:server",
    "build:server": "webpack --config webpack.server.js",
    "build:client": "webpack --config webpack.client.prod.js",
    "start": "NODE_ENV=production node dist/server.js"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.24.6",
    "css-loader": "^1.0.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.22.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "serve-static": "^1.13.2"
  }
}
